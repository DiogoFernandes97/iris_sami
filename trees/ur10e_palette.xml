        </Action>/>

        <Action ID="Grip"/>
        <Action ID="Release"/>

        <Action ID="Joints">
            <input_port name="shoulder_pan"     type="float"/>
            <input_port name="shoulder_lift"    type="float"/>
            <input_port name="elbow"            type="float"/>
